{
   "routes": [
      {
        "route": "/",
        "path": "index.html"
      },
      {
        "route": "/admin",
        "path": "admin.html",
        "allowedRoles": ["admin"]
      },
      { 
        "route": "/funding",
        "path": "funding.html",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/home",
        "path": "home.html",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/logout",
        "redirect": "/.auth/logout?post_logout_redirect_uri=/"
      }
    ],
    "responseOverrides": {
      "401": {
        "redirect": "/.auth/login/aad?post_login_redirect_uri=/home",
        "statusCode": 302
      }
    }

}